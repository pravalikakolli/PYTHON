a=int(input('enter a value:'))
b=int(input('enter b value:'))
if a<b:
    print('b is big')
else:
    print('a is big')
if a>b:
    print('a is big')
else:
    print('b is big')
if a==b:
    print('both are equal')
else:
    print('not equal')
if a!=b:
    print('not equal')
else:
    print('equal')
if a<=b:
    print('b greater than or equals to a')
else:
    print('a greatert than b')
if a>=b:
    print('a greater than or equals to b')
else:
    print('b greater than a')
    
    output:
    enter a value:13
enter b value:16
